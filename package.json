    "nano-memoize": "^1.0.3",
    "postcss-rtl": "^1.3.2",
    "prismjs": "^1.16.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-json-view": "^1.19.1",
    "react-jss": "^8.6.1",
