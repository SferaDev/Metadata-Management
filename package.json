    "d2-ui-components": "^0.0.15",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "font-awesome": "^4.7.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "markdown-table": "^1.1.3",
